<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="mathOnFingers.Pages.Page1">
    <ContentPage.Content>
        <StackLayout Padding="5,5,5,5" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
            <Label x:Name="selectlevelLable" Text="Выбери уровень сложности!" FontSize="46" FontAttributes="Bold"
                   TextColor="DarkSlateGray" HorizontalTextAlignment="Center"/>
